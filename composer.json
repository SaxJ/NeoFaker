{
    "name": "saxj/neofaker",
    "type": "library",
    "description": "NeoFaker is a PHP library that generates fake data for you.",
    "keywords": [
        "faker",
        "neofaker",
        "fixtures",
        "data"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Zaninotto"
        },
        {
            "name": "Saxon Jensen"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0"
    },
    "require-dev": {
        "ext-intl": "*",
        "phpunit/phpunit": "^4.8.35 || ^5.7",
        "squizlabs/php_codesniffer": "^2.9.2"
    },
    "autoload": {
        "psr-4": {
            "Faker\\": "src/NeoFaker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faker\\Test\\": "test/Faker/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.9-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
